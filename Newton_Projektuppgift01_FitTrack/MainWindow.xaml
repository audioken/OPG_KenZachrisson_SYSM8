<Window x:Class="Newton_Projektuppgift01_FitTrack.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Newton_Projektuppgift01_FitTrack"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Label Content="FitTrack"/>
        <TextBox Text="Username.."/>
        <TextBox Text="Password.."/>
        <Button Height="20" Content="Sign in" Command="{Binding SignInCommand}"/>
        <Button Height="20" Content="Register" Command="{Binding RegisterCommand}"/>
        
        <!-- För VG -->
        <Button Height="20" Content="Forgot Password" Command="{Binding ForgotPasswordCommand}"/>
    </StackPanel>
</Window>
